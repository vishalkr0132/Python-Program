    "import re\n",
    "def find(message,patt):\n",
    "    res = re.search(patt,message)\n",
    "    if res:\n",
    "        return 'Match Found'\n",
    "    return 'Not Match Found'\n",
    "\n",
    "message = 'The most violet man called one man the most violent'\n",
    "patt = input('Enter the searching words:')\n",
    "print(find(message, patt))"
   ]
